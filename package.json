{
  "name": "socialnetwork",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx ts-node build.ts",
    "start": "node dist/bin/www.js",
    "dev:start": "nodemon --exec ts-node src/bin/www.ts",
    "server:start": "SET DEBUG=Social_Network:* & npm run dev:start",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.{ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,json}",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migrate:generate": "npm run typeorm -- migration:generate -d ./src/config/data-source.ts",
    "migrate:run": "npm run typeorm -- migration:run -d ./src/config/data-source.ts",
    "migrate:revert": "npm run typeorm -- migration:revert -d ./src/config/data-source.ts",
    "migrate:show": "npm run typeorm -- migration:show -d ./src/config/data-source.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "faker": "^6.6.6",
    "fs-extra": "^11.2.0",
    "http-errors": "~2.0.0",
    "http-status-codes": "^2.3.0",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.3",
    "pug": "^3.0.3",
    "react": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/faker": "^6.6.9",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "description": "Project for learning Node.js Express tutorial.",
  "main": "src/bin/www.ts",
  "license": "ISC"
}
